@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>

<h1>Roles</h1>
<table class="table">
    <thead>
        <tr>
            <th>Role Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var role in Model)
        {
            <tr>
                <td>@role.Name</td>
            </tr>
        }
    </tbody>
</table>
<a class="btn btn-primary" href="/Roles/CreateRole">Create New Role</a>
<a class="btn btn-primary" href="/Roles/AssignRole">Assign Role to User</a>

