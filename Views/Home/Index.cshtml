    <section class="track_section layout_padding">
        <div class="track_bg_box">
            <img src="~/js/images/track-bg.jpg" alt="">

        </div>
        <div class="container">
            <div class="row">
                <div class="col-md-6">
                    <div class="heading_container">
                        <h2>
                            Track Your Shipment
                        </h2>
                    </div>
                    <p>
                        With [Parcel System], tracking your shipment is simple, fast, and hassle-free. Our advanced tracking system ensures that you stay informed at every step of your parcel’s journey, from pickup to delivery.
                    </p>
                    <!-- Search Bar -->
                    <div class="search-bar mb-4">
                        <form>
                            <div class="input-group">
                                <input type="text"
                                       name="searchTerm"
                                       class="form-control"
                                       placeholder="Enter Your Tracking Number....."
                                       value="@ViewData["SearchTerm"]" />
                                <button type="submit" class="btn btn-primary">
                                    Track
                                </button>
                            </div>
                        </form>
                    </div>

                    @if (Model != null)
                    {
                        <div class="parcel-details mt-4">
                            <h3>Parcel Details</h3>
                            <ul>
                                <li><strong>Tracking Number:</strong> @Model.TrackingNumber</li>
                                <li><strong>Status:</strong> @Model.Status</li>
                                <li><strong>Shipped Date:</strong> @Model.CreatedAt</li>
                                <li><strong>CreatedBy:</strong> @Model.CreatedBy</li>
                                <li><strong>Recipient Name:</strong> @Model.RecipientName</li>
                                <li><strong>Shipping Method:</strong> @Model.ShippingMethod</li>
                                <li><strong>Delivered At:</strong> @Model.DeliveredAt</li>
                            
                            </ul>
                        </div>
                    }
                    else if (ViewData["Message"] != null)
                    {
                        <div class="alert alert-warning mt-4">
                            @ViewData["Message"]
                        </div>
                    }


                      
                      
                </div>
            </div>
        </div>
    </section>

